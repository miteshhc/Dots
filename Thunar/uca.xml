<actions>
<action>
	<icon>x-package-repository</icon>
	<name>Create Archive</name>
	<submenu>7z</submenu>
	<unique-id>1713199684441771-1</unique-id>
	<command>7z a %n.7z %F</command>
	<description>Creates an archive(7z)</description>
	<range>*</range>
	<patterns>*</patterns>
	<directories/>
	<other-files/>
</action>
<action>
	<icon>x-package-repository</icon>
